# Copy to .env.local (gitignored) and fill with your project values.
# Backend cache defaults
CACHE_TTL_SECONDS=60
HTTP_TIMEOUT_SECONDS=8.0
HTTP_MAX_CONNECTIONS=12
ENABLE_PRECISE_MODE=false

# Infura credentials (auto-generate RPC endpoints like https://<network>.infura.io/v3/${INFURA_PROJECT_ID})
# Network slugs used: mainnet, polygon-mainnet, arbitrum-mainnet, optimism-mainnet, avalanche-mainnet, linea-mainnet
INFURA_PROJECT_ID=
INFURA_PROJECT_SECRET=

# Optional overrides for per-chain RPC endpoints (Alchemy, custom nodes, etc.)
RPC_ETHEREUM_URL=
RPC_POLYGON_URL=
RPC_ARBITRUM_URL=
RPC_OPTIMISM_URL=
RPC_AVALANCHE_URL=
RPC_LINEA_URL=

# Fee estimation overrides (optional)
ESTIMATE_FROM_ADDRESS=0x000000000000000000000000000000000000dead
ESTIMATE_TO_ADDRESS=0x000000000000000000000000000000000000beef
ESTIMATE_VALUE_WEI=1
